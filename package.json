{
  "name": "seller-supporter-be",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node App.js",
    "build:win-ia32": "electron-builder --win --ia32",
    "build:win-x64": "electron-builder --win --x64"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express": "^4.18.1",
    "puppeteer": "^21.7.0"
  },
  "devDependencies": {
    "electron": "^28.2.2",
    "electron-builder": "^24.9.1"
  },
  "author": {
    "name": "Seller-Supporter ManageMent"
  },
  "build": {
    "productName": "Seller-Supporter",
    "files": [
      "build/**/*",
      "src/**/*",
      "main.js",
      "App.js"
    ],
    "asar": false,
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "shortcutName": "Seller-Supporter",
      "artifactName": "Seller-Supporter ${version}.${ext}",
      "uninstallDisplayName": "Seller-Supporter"
    }
  }
}
